# single potential calculations on one temperature
# also testing the random number generator
# moving all elements

&GLOBAL
  PROJECT H2O_TMC
  PROGRAM TMC
  RUN_TYPE TMC
  PRINT_LEVEL LOW
  WALLTIME 00:10:25 
&END GLOBAL
&MOTION
  &TMC
      GROUP_ENERGY_SIZE 1
      GROUP_ENERGY_NR 1
      GROUP_CC_SIZE 0
      NUM_MC_ELEM 100
      ENERGY_FILE_NAME H2O.inp
      TEMPERATURE 330
      MOVE_TYPES 1 2 3
      MOVE_SIZES 0.1 0.7 24
      NUM_MV_ELEM_IN_CELL 0
      RND_DETERMINISTIC 42
      START_ACC_PROB 0.2
      ESIMATE_ACC_PROB .TRUE.
      INFO_OUT_STEP_SIZE 1
      RESTART_OUT 0
      PRINT_ONLY_ACC .TRUE.
      PRINT_TEST_OUTPUT
  &END TMC
&END MOTION
